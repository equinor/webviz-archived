    "@babel/preset-env": "^7.0.0",
    "@babel/register": "^7.0.0",
    "babel-eslint": "^9.0.0",
    "babelify": "^9.0.0",
    "browserify": "^16.2.2",
    "chai": "^4.1.2",
    "eslint": "^5.5.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-chai-expect": "^1.1.1",
    "eslint-plugin-import": "^2.14.0",
    "ify-loader": "^1.1.0",
    "jsdom": "^12.0.0",
    "jsdom-global": "^3.0.2",
